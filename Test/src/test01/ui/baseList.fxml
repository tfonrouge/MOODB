<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="1200.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea">
    <center>
        <TableView fx:id="tableView" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
            <columns>
                <TableColumn prefWidth="75.0" text="Column X"/>
            </columns>
        </TableView>
    </center>
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <Menu mnemonicParsing="false" text="Archivo">
                <MenuItem fx:id="menuItem_close" mnemonicParsing="false" text="Close"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Editar">
                <MenuItem fx:id="menuItem_insert" mnemonicParsing="false" onAction="#onActionInsertDocument"
                          text="Agregar"/>
                <MenuItem fx:id="menuItem_edit" mnemonicParsing="false" onAction="#onActionEditDocument"
                          text="Modificar"/>
                <MenuItem fx:id="menuItem_delete" mnemonicParsing="false" onAction="#onActionDeleteDocument"
                          text="Eliminar"/>
            </Menu>
            <Menu mnemonicParsing="false" text="View">
                <MenuItem fx:id="menuItem_view" mnemonicParsing="false" onAction="#onActionViewDocument"
                          text="View Document"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Ayuda">
                <MenuItem mnemonicParsing="false" text="About"/>
            </Menu>
        </MenuBar>
    </top>
    <left>
        <Accordion BorderPane.alignment="CENTER">
            <panes>
                <TitledPane animated="false" text="untitled 1">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                </TitledPane>
                <TitledPane animated="false" text="untitled 2">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                </TitledPane>
                <TitledPane animated="false" text="untitled 3">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                </TitledPane>
            </panes>
        </Accordion>
    </left>
    <bottom>
    </bottom>
</BorderPane>
